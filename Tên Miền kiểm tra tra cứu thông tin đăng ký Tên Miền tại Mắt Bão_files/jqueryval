function OnRegisterSuccess(n){$("#registerLoading").hide();$("#register_submit").show();n.ErrorType==3?alert("Mã bảo vệ không đúng!"):n.ErrorType==2?alert("Thông tin không hợp lệ, vui lòng kiểm tra lại!"):n.ErrorType==1?alert("Tạo tài khoản lỗi, vui lòng thử lại!"):location.href="/thanh-vien/kich-hoat-tai-khoan.html"}function OnRegisterFailure(){alert("Có lỗi hệ thống xảy ra, vui lòng thử lại!");$("#registerLoading").hide();$("#register_submit").show()}function OnLoginFailure(){alert("Có lỗi hệ thống xảy ra, vui lòng thử lại!");$("#LoginLoading").hide();$("#btn_login_submit").show()}function OnLoginSuccess(n){$("#LoginLoading").hide();$("#btn_login_submit").show();n.ErrorType==1?alert("Tên đăng nhập hoặc mật khẩu không đúng!"):n.ErrorType==2?(alert("Tài khoản chưa được kích hoạt!"),location.href="/thanh-vien/lay-lai-ma-kich-hoat-tai-khoan.html"):n.ErrorType==3?alert("Tài khoản đang tạm khóa, vui lòng liên hệ tổng đài để được giải quyết!"):location.href=location.href}function submitLogin(){$("#form-login").valid()&&$("#btn_login_submit").hide()}function submitRegister(){$("#form-register").valid()&&$("#register_submit").hide()}function OnActiveAccountSuccess(n){$("#ActiveAccountLoading").hide();$("#active_submit").show();n.ErrorType==0?(alert("Tài khoản đã được kích hoạt!"),location.href="/"):n.ErrorType==1&&alert("Mã xác nhận không đúng. Vui lòng kiểm tra lại!");n.ErrorType==2&&alert("Mã xác nhận đã hết hạn, vui lòng liên hệ ban quản trị để được hỗ trợ!")}function OnActiveAccountFailure(){alert("Có lỗi hệ thống xảy ra, vui lòng thử lại!");$("#ActiveAccountLoading").hide();$("#active_submit").show()}function OnResendCodeActiveAccountSuccess(n){$("#ResendCodeActiveAccountLoading").hide();$("#resend_code_active_submit").show();n.ErrorType==0?(alert("Mã xác nhận mới đã được gửi!"),location.href="/thanh-vien/kich-hoat-tai-khoan.html"):alert("Có lỗi hệ thống xảy ra, vui lòng thử lại!")}function OnResendCodeActiveAccountFailure(){alert("Có lỗi hệ thống xảy ra, vui lòng thử